ListGroup.Item>\r\n              <ListGroup.Item>Price: ${product.price}</ListGroup.Item>\r\n              <ListGroup.Item>\r\n                Description: {product.description}\r\n              </ListGroup.Item>\r\n            </ListGroup>\r\n          </Col>\r\n\r\n          <Col md={3}>\r\n            <Card>\r\n              <ListGroup variant='flush'>\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Price:</Col>\r\n                    <Col>\r\n                      <strong>${product.price}</strong>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Status:</Col>\r\n                    <Col>\r\n                      {product.countInStock > 0 ? 'In Stock' : 'Out Of Stock'}\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n                {product.countInStock > 0 && (\r\n                  <ListGroup.Item>\r\n                    <Row>\r\n                      <Col>Qty</Col>\r\n                      <Col>\r\n                        <Form.Control\r\n                          as='select'\r\n                          value={qty}\r\n                          onChange={(e) => setQty(e.target.value)}\r\n                        >\r\n                          {[...Array(product.countInStock).keys()].map((x) => (\r\n                            <option key={x + 1} value={x + 1}>\r\n                              {x + 1}\r\n                            </option>\r\n                          ))}\r\n                        </Form.Control>\r\n                      </Col>\r\n                    </Row>\r\n                  </ListGroup.Item>\r\n                )}\r\n                <ListGroup.Item>\r\n                  <Button\r\n                    onClick={addToCartHandler}\r\n                    className='btn btn-block'\r\n                    disabled={product.countInStock === 0}\r\n                  >\r\n                    Add To Cart\r\n                  </Button>\r\n                </ListGroup.Item>\r\n              </ListGroup>\r\n            </Card>\r\n          </Col>\r\n        </Row>\r\n      )}\r\n    </>\r\n  )\r\n}\r\n\r\nexport default ProductScreen\r\n","C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\components\\Header.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\components\\Product.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\components\\Rating.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\reducers\\productReducers.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\actions\\productActions.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\constants\\productConstants.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\components\\Loader.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\components\\Message.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\screens\\CartScreen.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\reducers\\cartReducers.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\actions\\cartActions.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\reducers\\userReducers.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\constants\\userConstants.js",[],["162","163"],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\screens\\LoginScreen.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\components\\FormContainer.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\actions\\userActions.js",[],"C:\\Users\\csg91\\OneDrive\\Desktop\\e-commarce\\frontend\\src\\scr